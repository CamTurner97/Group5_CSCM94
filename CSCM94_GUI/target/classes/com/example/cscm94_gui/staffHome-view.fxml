<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.control.Button?>

<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.cscm94_gui.StaffHomeController">

    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <ImageView fitHeight="64" fitWidth="64">
        <image>
            <Image url="https://img.freepik.com/free-vector/waiter-serving-wine-flat-vector-illustration-young-boy-apron-holding-tray-with-wineglasses-cartoon-character-experienced-restaurant-staff-member-cafe-employee-male-catering-service-worker_198278-6210.jpg" />
        </image>
    </ImageView>

    <Label>Staff Homepage</Label>

    <Button fx:id="ManageStaffButton" text="Manage Staff (Manager)" onAction="#onManageStaffButtonClick" />

    <Button fx:id="ManageOrdersButton" text="Manage Orders (Waiters/Chefs)" onAction="#onManageOrdersButtonClick"/>

    <Button fx:id="ManageDeliveryButton" text="Manage Delivery (Waiters/Chefs/Delivery Drivers)" onAction="#onManageDeliveryButtonClick"/>

    <Button text="Approve Bookings (Waiters)" />

    <Button fx:id="LogOutButton" text="Log out" onAction="#onLogOutButtonClick" />




</VBox>

