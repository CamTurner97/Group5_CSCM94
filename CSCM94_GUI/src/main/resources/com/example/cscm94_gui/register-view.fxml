<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.TextField?>

<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.cscm94_gui.registerController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <ImageView fitHeight="64" fitWidth="64">
        <image>
            <Image url="https://image.shutterstock.com/image-vector/coffee-cup-character-design-cartoon-260nw-1888372606.jpg" />
        </image>
    </ImageView>

    <Label fx:id="EnterNameLabel">Enter Name</Label>
    <TextField minWidth="180.0" maxWidth="180.0"></TextField>

    <Label fx:id="EnterUsername">Enter Username</Label>
    <TextField minWidth="180.0" maxWidth="180.0"></TextField>

    <Label fx:id="EnterPassword">Enter Password</Label>
    <TextField minWidth="180.0" maxWidth="180.0"></TextField>

    <Button fx:id="RegisterConfirmButton" text="Confirm Registration" />

</VBox>

